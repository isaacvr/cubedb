{
  "productName": "cube-db",
  "appId": "com.isaacvr.cubedb",
  "compression": "maximum",
  "electronDownload": {
    "cache": "D:\\STUDY\\programacion\\WORK\\CubeDB\\electron-cache",
    "isVerifyChecksum": false,
    "platform": "win32",
    "arch": "x64"
  },
  "asar": false,
  "directories": {
    "output": "release/"
  },
    "files": [
        "**/*",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/",
        "!scrappers/",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json"
    ],
  "win": {
    "icon": "src/assets/icons/icon.ico",
    "target": [
      "nsis"
    ]
  },
  "mac": {
    "icon": "dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}
